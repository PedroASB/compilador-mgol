{ERROS LEXICOS, SINTATICOS E SEMANTICOS}

inicio
    iniciovar {Erro sintatico - 'iniciovar' no lugar de 'varinicio'}
        literal A;
        inteiro B, D, E;
        real C;
    varfim;
    escreva "Digite B:";
    leia B; @ {Erro lexico}
    escreva "Digite A:";
    C<-A+2.0; {Erro semantico}
    leia A;
    se (B>2) entao
        se (B<=4) entao
            escreva "B esta entre 2 e 4";
        fimse
    fimse
    B<-3.0; {Erro semantico}
    B<-B+2;
    B<-B+3;
    D<-B; asd489fk {Erro lexico}
    C<-5.0;
    repita (B - 5) {Erro sintatico - '-' no lugar de <EXP_R>}
        C<-B+2;
        escreva C;
        B<-B+1;
    fimse {Erro sintatico - 'fimse' no lugar de 'fimrepita'}
    escreva "\nB=\n";
    escreva D;
    escreva "\n";
    escreva C;
    escreva "\n";
    escreva A;
fim