{ERROS SINTATICOS - TOKENS EM EXCESSO}

inicio
   varinicio
        literal A;
        inteiro B, D,, E; {',' em excesso}
        real C;
    varfim;
    escreva "Digite B:";
    leia A A; {'A' em excesso}
    escreva "Digite A:";
    leia A;
    C <- B ++ 2; {'+' em excesso}
    escreva C;
    escreva A;
    B <- E * 5;; {';' em excesso}
fim